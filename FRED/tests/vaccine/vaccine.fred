
# SINGLE-DOSE INFLUENZA VACCINE MODEL

#### CONDITIONS
use FRED::Influenza

Condition VAC

## Properties of VAC:
VAC.states = Start Unavail Avail Receive Immune Failed ImportStart ImportVaccine
VAC.import_start_state = ImportStart
VAC.exposed_state = Avail


if state(VAC,Start) {
  effect()
  wait(0)
  next(Unavail)
} # end of state(VAC,Start)

if state(VAC,Unavail) {
  set_sus(VAC,1)
  wait()
  next()
} # end of state(VAC,Unavail)

if state(VAC,Avail) {
  set_sus(VAC,0)
  wait(24*lognormal(10,2))
  next(Receive)
} # end of state(VAC,Avail)

if state(VAC,Receive) {
  effect()
  wait(24*14)
  next(Immune) with prob(0.8)
  default(Failed)
} # end of state(VAC,Receive)

if state(VAC,Immune) {
  set_sus(INF,0.0)
  wait()
  next()
} # end of state(VAC,Immune)

if state(VAC,Failed) {
  effect()
  wait()
  next()
} # end of state(VAC,Failed)

if state(VAC,ImportStart) {
  effect()
  wait(0)
  next(ImportVaccine)
} # end of state(VAC,ImportStart)

if state(VAC,ImportVaccine) {
  import_per_capita(0.30)
  wait()
  next()
} # end of state(VAC,ImportVaccine)

